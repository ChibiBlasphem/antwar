<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title><%= webpackConfig.template.title %></title>
    <% if (webpackConfig.template.rss) { %>
    <link rel="alternate" type="application/atom+xml" href="<%= webpackConfig.template.rss.href %>" title="<%= webpackConfig.template.rss.title %>">
    <% } %>

    <% for (var file of webpackConfig.template.cssFiles) { %>
      <link href="<%= file %>" rel="stylesheet">
    <% } %>
  </head>
  <body>
    <%- webpackConfig.html %>

    <% for (var script of webpackConfig.template.jsFiles) { %>
      <script src="<%= script %>" type="text/javascript"></script>
    <% } %>
  </body>
</html>
